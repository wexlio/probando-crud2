<div class="container pb-4 pl-0">
    <h1>All Productest</h1>
</div>


<table class="table">
  <thead>
    <tr class="bg-success">
      <th class="text-center" scope="col">#</th>
      <th class="text-center" scope="col">Title</th>
      <th class="text-center" scope="col">Price</th>
      <th class="text-center" scope="col">Actions</th>
    </tr>
  </thead>
  <tbody>
    {{#each product}}
    <tr>
      <th class="text-center" scope="row">1</th>
      <td class="text-center">{{title}}</td>
      <td class="text-center">{{price}}</td>
      <td class="text-center">
        <button type="button" class="btn btn-primary mr-2">Edit</button>
        <button type="button" class="btn btn-success mr-2">ViewImg</button>
        <button type="button" class="btn btn-danger mr-2">Delete</button>
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>